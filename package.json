{
  "name": "n8n-nodes-quoter",
  "version": "0.1.0",
  "description": "n8n community node for Quoter API",
  "keywords": [
    "n8n-community-node-package",
    "quoter",
    "quoting",
    "msp",
    "quotes"
  ],
  "license": "MIT",
  "homepage": "https://github.com/YOUR_USERNAME/n8n-nodes-quoter",
  "repository": {
    "type": "git",
    "url": "https://github.com/YOUR_USERNAME/n8n-nodes-quoter.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts",
    "lintfix": "eslint . --ext .ts --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/QuoterApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Quoter/Quoter.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^2.8.0",
    "typescript": "^4.9.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
